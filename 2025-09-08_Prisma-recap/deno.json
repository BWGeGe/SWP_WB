{
  "tasks": {
    "dev": "deno run --watch main.ts",
    "seed": "deno run seed.ts",
    "p:generate": "deno run -A npm:prisma@latest generate",
    "p:migrate": "deno run -A npm:prisma@latest migrate dev",
    "p:studio": "deno run -A npm:prisma@latest studio"
  },
  "imports": {
    "@prisma/client": "npm:@prisma/client@^6.16.2",
    "@std/assert": "jsr:@std/assert@1"
  }
}
